{
	"name" : "mbarbey/u2f-security-bundle",
	"type" : "symfony-bundle",
	"description" : "Symfony bundle allowing to use U2F security keys as two factor authentication",
	"homepage" : "https://github.com/mbarbey/u2f-security-bundle",
	"license" : "MIT",
	"keywords": ["security", "u2f", "two factor", "authentication", "yubikey"],
	"authors" : [
		{
			"name" : "Michael Barbey",
			"email" : "michael@barbey-family.ch"
		}
	],
	"require": {
	    "php": "^5.5.9 || ^7.0",
		"samyoul/u2f-php-server": "^1.1",
		"symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0",
		"symfony/security-bundle": "^2.8 || ^3.0 || ^4.0",
		"symfony/doctrine-bridge": "^2.8 || ^3.0 || ^4.0",
		"symfony/validator": "^2.8 || ^3.0 || ^4.0"
	},
	"require-dev": {
		"symfony/phpunit-bridge": "^4.8.35 || ^5.7.11 || ^6.5",
		"symfony/yaml": "^2.8 || ^3.0 || ^4.0",
		"phpunit/phpunit": "^4.8.35 || ^5.7.11 || ^6.5"
	},
	"autoload" : {
		"psr-4" : {
			"Mbarbey\\U2fSecurityBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"autoload-dev": {
        "psr-4": {
            "Mbarbey\\U2fSecurityBundle\\Tests\\": "/Tests/"
        }
	}
}
